{
  "datasets": [
    {
      "name": "30f00002",
      "displayName": "Location Data",
      "queryLines": [
        "SELECT latitude,\r\n",
        "longitude,\r\n",
        "'GHG emissions mtons CO2e' as Emissions\r\n",
        "FROM emissions.default.emissions_data\r\n"
      ]
    },
    {
      "name": "6e6db2cc",
      "displayName": "Emissions Per Person",
      "queryLines": [
        "SELECT county_state_name,\r\n",
        "       population,\r\n",
        "       CAST(\r\n",
        "         REPLACE(`GHG emissions mtons CO2e`, ',', '')\r\n",
        "         AS DOUBLE\r\n",
        "       ) / nullif(\r\n",
        "        cast(\r\n",
        "         REPLACE(population, ',', '')\r\n",
        "         AS DOUBLE\r\n",
        "       ), 0\r\n",
        "       ) AS emissions_per_person\r\n",
        "FROM emissions.default.emissions_data\r\n",
        "ORDER BY emissions_per_person DESC\r\n"
      ],
      "catalog": "emissions",
      "schema": "default"
    },
    {
      "name": "d4860562",
      "displayName": "Total Emissions Per State",
      "queryLines": [
        "SELECT state_abbr,\r\n",
        "       SUM (\r\n",
        "         CAST(\r\n",
        "           REPLACE(\r\n",
        "             `GHG emissions mtons CO2e`,\r\n",
        "             ',',\r\n",
        "             ''\r\n",
        "           )\r\n",
        "           AS DOUBLE\r\n",
        "         )\r\n",
        "       ) AS Total_Emissions\r\n",
        "FROM emissions_data\r\n",
        "GROUP BY state_abbr\r\n",
        "ORDER BY Total_Emissions DESC\r\n",
        "LIMIT 10"
      ],
      "catalog": "emissions",
      "schema": "default"
    },
    {
      "name": "03d7b941",
      "displayName": "County Shaming",
      "queryLines": [
        "SELECT county_state_name,\r\n",
        "       population,\r\n",
        "       CAST(\r\n",
        "         REPLACE(`GHG emissions mtons CO2e`, ',', '')\r\n",
        "         AS DOUBLE) as total_emissions\r\n",
        "         FROM emissions_data\r\n",
        "ORDER BY total_emissions DESC\r\n",
        "LIMIT 10"
      ],
      "catalog": "emissions",
      "schema": "default"
    }
  ],
